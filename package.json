{
  "name": "potree-core",
  "version": "1.4.13",
  "description": "Potree wrapper for Three.js applications",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/contilio/potree-core.git"
  },
  "scripts": {
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "docs": "jsdoc -d docs source",
    "prebump": "yarn version --no-git-tag-version && yarn",
    "bump": "yarn bump:commit && yarn bump:tag && yarn bump:push",
    "bump:commit": "git add -A && cross-var git commit -m \"chore: bump version $npm_package_version\"",
    "bump:tag": "cross-var git tag v$npm_package_version",
    "bump:push": "git push -u origin head && git push --tags",
    "release": "yarn build && yarn publish --non-interactive"
  },
  "keywords": [
    "three",
    "potree",
    "3d",
    "webgl"
  ],
  "files": [
    "dist"
  ],
  "types": "types/Main.d.ts",
  "author": "Tentone",
  "license": "MIT",
  "devDependencies": {
    "@types/three": "^0.151.0",
    "cross-var": "^1.1.0",
    "jsdoc": "^3.5.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "three": "^0.151.3"
  }
}